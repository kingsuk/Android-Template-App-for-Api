<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="WE THINK, WE LISTEN, WE CARE"
        android:id="@+id/textView6"
        android:gravity="center"
        android:textColor="@color/dash_text_color"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="AREAS OF EXPERTISE"
        android:id="@+id/textView7"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/dash_text_color"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="We cover every aspect of a domestic concierge servcice."
        android:id="@+id/textView8"
        android:textStyle="italic"
        android:textSize="13dp"
        android:textColor="@color/dash_text_color"
        android:gravity="center"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Serving NYC online for over 10 years."
        android:id="@+id/textView9"
        android:textSize="10dp"
        android:textColor="@color/dash_text_color"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:paddingBottom="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Fine Dry Cleaning and Laundry Service in New York City - Straight to Your Door"
        android:id="@+id/textView10"
        android:textStyle="bold"
        android:textColor="@color/dash_text_color"
        android:paddingTop="15dp"
        android:paddingBottom="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Owned and Operated facility in Manhattan"
        android:id="@+id/textView11"
        android:textStyle="italic|bold"
        android:textColor="@color/dash_text_color"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Organic cleaning, offering our clients wet cleaning service in New York City (NYC), true Eco-friendly cleaning. Free pick-up and delivery on all dry cleaning and wash and fold service in NYC. The finest Wash &amp; Fold, laundry Service in New York City. Expert stain removal and restoration for your fine garments. Each garment is personally inspected and treated for the highest quality. We clean the finest leather and suede. We offer you full storage on all your clothes including a fur vault."
        android:id="@+id/textView12"
        android:textColor="@color/dash_text_color"
        android:textSize="10dp"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Corporate"
        android:id="@+id/textView13"
        android:textColor="@color/dash_text_color"
        android:paddingTop="15dp"
        android:textStyle="bold"
        android:paddingBottom="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="All Coporate Events from Catering and Uniforms to Large Sports Events"
        android:id="@+id/textView14"
        android:textColor="@color/dash_text_color"
        android:textStyle="italic|bold"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Dry Cleaning service for staff and buildings Work directly with property management companies and property mangers Laundry and dry cleaning service for corporate events and special events Executive Wash &amp; Fold service for nursery schools, Hedge funds, Executive gyms and corporate companies."
        android:id="@+id/textView16"
        android:textSize="10dp"
        android:textColor="@color/dash_text_color"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"/>
</LinearLayout>